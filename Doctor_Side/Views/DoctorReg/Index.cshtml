@model IEnumerable<Doctor_Side.Models.DoctorReg>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Contact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Degree)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Profile)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Degree)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Profile)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Doctor_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Doctor_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Doctor_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
